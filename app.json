{
  "expo": {
    "name": "Our Edu App Test",
    "slug": "react-native-app",
    "owner": "abdelrahmankamel",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/AppIcon.png",
    "splash": {
      "image": "./src/assets/images/AppIcon.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "plugins": [
  "expo-localization",
  [
    "expo-font",
    {
      "fonts": ["./assets/fonts/Alexandria-Bold.ttf"]
    }
  ],
  [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 28,
            "targetSdkVersion": 35,
            "compileSdkVersion": 35
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "android": {
      "package": "com.abdelrahmankamel.reactnativeapp",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/AppIcon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE"
      ],
      "blockedPermissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "ouredu.onrender.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
       
      ],
      "userInterfaceStyle": "dark",
      "softwareKeyboardLayoutMode": "pan",
      "runtimeVersion": "1.0.0"
    },
    "ios": {
      "bundleIdentifier": "com.abdelrahmankamel.reactnativeapp",
      "buildNumber": "1.0.0",
      "supportsTablet": false,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCameraUsageDescription": "We need camera access to upload your profile photo.",
        "NSPhotoLibraryUsageDescription": "We need photo library access to select images.",
        "NSLocationWhenInUseUsageDescription": "We use your location to improve your experience."
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "extra": {
      "eas": {
        "projectId": "4f1b5faf-5233-4407-8c40-ec8f5e24325e"
      }
    }
  }
}
