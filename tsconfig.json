{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    },
    "types": ["react", "react-dom", "react-native", "node"]
  },
"include": ["src", "App.tsx", "App.js", "index.ts", "index.js", "*.d.ts"],
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
